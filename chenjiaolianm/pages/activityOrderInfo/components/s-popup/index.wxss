}
.s-popup.s-popup-visible {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.s-popup .s-popup-mask {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  pointer-events: auto;
}
.s-popup .s-popup-wrapper {
