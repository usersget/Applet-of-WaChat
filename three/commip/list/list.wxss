/* commip/list/list.wxss */
.list{
  width: 100%;
  height: 350rpx;
  display: flex;
}
.list_left{
  width: 70%;
  height: 90%;
  padding: 30px 0;
  box-sizing: border-box;
}
.list_right{
  width: 30%;
  height: 90%;
  box-sizing: border-box;
}
.list_right image{
  width: 160rpx;
  height: 250rpx;
  margin-top: 5px;
  margin-left: 5px;
}
.list text{
  display: block;
  line-height: 19px;
  font-size: 14px;
}
